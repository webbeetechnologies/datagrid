{
  "name": "@bambooapp/datagrid",
  "version": "0.4.4",
  "author": "Thet Aung <thetaung.dev@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@3.3.0",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib",
    "src",
    "canvas",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "start": "pnpm storybook",
    "storybook": "MOLECULES_ENV=storybook start-storybook dev -p 6006",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepare:husky": "husky install",
    "test-jest": "jest",
    "prepare": "bob build",
    "bundle": "rm -rf lib && pnpm prepare && mkdir -p dist && npm pack --pack-destination=dist",
    "do-release": "npm publish ./dist/bambooapp-datagrid-$npm_package_version.tgz --access=public",
    "release": "pnpm bundle && pnpm do-release",
    "release:dev": "npm version prerelease --preid=dev && pnpm release --tag=dev",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@shopify/react-native-skia": "2.0.0-next.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.keyby": "^4.6.7",
    "@types/lodash.memoize": "^4.1.7",
    "@types/lodash.noop": "^3.0.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.omitby": "^4.6.7",
    "@types/use-sync-external-store": "^0.0.3",
    "@types/react": "~19.0.14",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.19.9",
    "babel-preset-expo": "^12.0.11",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.4.0",
    "jest-environment-node": "27",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.7.1",
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-builder-bob": "^0.17.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.3.0",
    "ts-jest": "^29.0.3",
    "typescript": "~4.9.5"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {}
      ],
      [
        "module",
        {}
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "test-cases/node_modules",
    "dist",
    "cache"
  ],
  "dependencies": {
    "@bambooapp/react-hooks": "^1.0.9",
    "use-sync-external-store": "^1.2.0",
    "color": "^4.2.3",
    "eventemitter2": "^6.4.9",
    "lodash.isnil": "^4.0.0",
    "lodash.memoize": "^4.1.2",
    "lodash.debounce": "^4.0.8",
    "grapheme-splitter": "^1.0.4",
    "konva": "^9.2.3",
    "lru-cache": "^11.0.2",
    "react-konva": "^18.2.10",
    "scroller": "^0.0.3",
    "tiny-invariant": "^1.3.1"
  }
}
